  "description": "Prisma Client extension for pagination",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
